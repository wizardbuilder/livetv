<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.wizard" name="Wizard" version="1.0.4" provider-name="Wizard">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
		<summary lang="en">Wizard</summary>
		<description lang="en">Easy Install of Builds and Addons</description>
		<disclaimer lang="en">Note: we are not responsible or have any influence to the content and their availability from Kodi Add-ons</disclaimer>
		<platform>all</platform>
		<facebook>-</facebook>
		<website>-</website>
	</extension>
</addon>

<addon id="plugin.video.livetv"
       name="IPTV"
       version="1.0.6~beta19"
       provider-name="wizardbuilder">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="run.py">
        <provides>video</provides>
    </extension>
	<extension point="xbmc.service" library="service.py" start="login"/>
    <extension point="xbmc.addon.metadata">
    <summary lang="en">LiveTv</summary>
    <description lang="en">LiveTV</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.wizardbuilder" name="Easyuse Repository" version="2.1" provider-name="wizardbuilder">
	<extension point="xbmc.addon.repository" name="wizardbuilder">
		<info compressed="false">https://raw.githubusercontent.com/wizardbuilder/livetv/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/wizardbuilder/livetv/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/wizardbuilder/livetv/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Wizard Builder Repo</summary>
		<description>Wizard Builder provides you the best solution for your kodi platform</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.module.urlresolver" name="URLResolver" version="4.0.09" provider-name="Eldorado, tknorris, jsergio">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" optional="true" />
		<import addon="script.module.python.twitch" optional="true" />
		<import addon="script.module.cryptopy" version="1.2.6" optional="true" />
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.python.pluginsource" library="lib/default.py" />
	<extension point="xbmc.addon.metadata">
	<extension point="xbmc.service" library="service.py" start="login"/>
		<platform>all</platform>
		<summary lang="en">Resolve common video host URL's to be playable in XBMC/Kodi.</summary>
		<summary lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju.</summary>
		<description lang="en">Resolve common video host URL's to be playable in XBMC/Kodi, simplify addon development of video plugins requiring multi video hosts.</description>
		<description lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju, pojednostavnjuje razvoj priključaka za video dodatke koji zahtjevaju višestruke video izvore.</description>
        <source>https://github.com/jsergio123/script.module.urlresolver</source>
	</extension>
</addon>

<addon id="script.service.patchbot"
       name="Patch Bot kodi 17 gebruik alleen voor windows IOS raspberry pi"
       version="0.0.9"
       provider-name="Josh.5">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.service" library="service.py" start="login"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Auto-sync system patches</summary>
    <description lang="en">Monitor and automatically install patches generated remotely via System Sync.</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
